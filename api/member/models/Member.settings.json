{
  "connection": "default",
  "collectionName": "members",
  "info": {
    "name": "member",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "lastname": {
      "type": "string"
    },
    "firstname": {
      "type": "string"
    },
    "pseudo": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "birthday": {
      "type": "date"
    },
    "phonenumber": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "address": {
      "type": "string"
    },
    "town": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "password": {
      "type": "password"
    },
    "registrationDate": {
      "type": "date"
    },
    "walletAmount": {
      "type": "integer"
    },
    "zip": {
      "type": "string"
    },
    "role": {
      "model": "role",
      "plugin": "users-permissions"
    }
  }
}